<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Overhaul</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/BiomeDef[defName = "DankPyon_DarkForest"]/wildPlants</xpath>
					<value>
						<Plant_AYAloeVera>0.03</Plant_AYAloeVera>
						<Plant_AYYarrow>0.03</Plant_AYYarrow>
					</value>
				</li>
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/BiomeDef[defName = "DankPyon_AncientForest"]/wildPlants</xpath>
					<value>
						<Plant_AYAloeVera>0.03</Plant_AYAloeVera>
						<Plant_AYYarrow>0.03</Plant_AYYarrow>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

